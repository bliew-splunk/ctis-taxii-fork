    "TAXII v2.1"
  ],
  "logo": "logo_splunk.svg",
  "logo_dark": "logo_splunk_dark.svg",
  "license": "Copyright (c) 2016-2024 Splunk Inc.",
  "pip_dependencies": {
    "pypi": [
      {
        "module": "taxii2-client>=2.3.0"
      },
      {
        "module": "stix2>=3.0.1"
      },
      {
        "module": "dataclasses-json>=0.6.7"
      }
    ]
  },
  "configuration": {
    "base_url": {
      "description": "Base URL of the TAXII REST Server",
      "order": 0,
      "data_type": "string"
    },
    "username": {
      "description": "Username",
      "order": 1,
      "data_type": "string"
    },
    "password": {
      "description": "Password",
      "order": 2,
      "data_type": "password"
    },
    "export_tag": {
      "description": "Scheduled submission: tag to filter for indicators to export to CTIS",
      "data_type": "string",
      "required": false,
      "default": "ctis_export",
      "order": 3
    }
  },
  "actions": [
    {
      "action": "test connectivity",
      "description": "Validate the asset configuration for connectivity",
      "type": "test",
      "identifier": "test_connectivity",
      "read_only": true,
      "parameters": {},
      "output": [],
      "versions": "EQ(*)"
    },
    {
      "action": "add soar indicator to stix bundle container",
      "description": "For a SOAR indicator, convert related artifacts CEF data to a STIX Indicator and add this to existing STIX bundle",
      "type": "generic",
      "identifier": "add_soar_indicator_to_stix_bundle",
      "read_only": false,
      "parameters": {
        "bundle_id": {
          "order": 0,
          "required": true,
          "description": "STIX Bundle ID",
          "data_type": "string"
        },
        "indicator_id": {
          "order": 1,
          "required": true,
          "description": "SOAR Indicator ID",
          "data_type": "string"
        },
        "tlp_rating": {
          "order": 2,
          "required": true,
          "default": null,
          "description": "Traffic Light Protocol (TLP) Rating",
          "data_type": "string",
          "value_list": [
            "WHITE",
            "GREEN",
            "AMBER",
            "RED"
          ]
        },
        "created_by_ref": {
          "order": 3,
          "required": true,
          "description": "The Identity ID of the creator of the Indicator",
          "data_type": "string"
        },
        "description": {
          "order": 4,
          "required": false,
          "description": "Description of the Indicator",
          "data_type": "string"
        },
        "confidence": {
          "order": 5,
          "required": false,
          "description": "Confidence value (0-100)",
          "data_type": "numeric"
        },
        "lang": {
          "order": 6,
          "required": false,
          "default": "en",
          "description": "Description of the Indicator",
          "data_type": "string"
        }
      },
      "output": [
        {
          "data_path": "action_result.data.*.json",
          "data_type": "string"
        },
        {
          "data_path": "action_result.data.*.stix_id",
          "data_type": "string"
        }
      ],
      "versions": "EQ(*)"
    },
    {
      "action": "get stix bundle container json",
      "description": "Get STIX bundle JSON by bundle ID",
      "type": "generic",
      "identifier": "get_stix_bundle",
      "read_only": true,
      "parameters": {
        "bundle_id": {
          "order": 0,
          "required": true,
          "description": "Bundle ID",
          "data_type": "string"
        }
      },
      "output": [
        {
          "data_path": "action_result.data.*.id",
          "data_type": "string"
        },
        {
          "data_path": "action_result.data.*.type",
          "data_type": "string"
        },
        {
          "data_path": "action_result.data.*.objects",
          "data_type": "string"
        }
      ],
      "versions": "EQ(*)"
    },
    {
      "action": "create stix bundle container",
      "description": "Create empty STIX bundle container",
      "type": "generic",
      "identifier": "create_stix_bundle_container",
      "read_only": false,
      "parameters": {},
      "output": [
        {
          "data_path": "action_result.data.*.container_id",
          "data_type": "string"
        },
        {
          "data_path": "action_result.data.*.bundle_id",
          "data_type": "string"
        }
      ],
      "versions": "EQ(*)"
    },
    {
      "action": "add identity to stix bundle container",
      "description": "Add an identity to a STIX bundle container",
      "type": "generic",
      "identifier": "add_identity_to_stix_bundle_container",
      "read_only": false,
      "parameters": {
        "bundle_id": {
          "order": 0,
          "required": true,
          "description": "Bundle ID",
          "data_type": "string"
        },
        "identity_id": {
          "order": 1,
          "required": true,
          "description": "Identity ID",
          "data_type": "string"
        },
        "identity_class": {
          "order": 2,
          "required": true,
          "default": "organization",
          "description": "Identity Class",
          "data_type": "string",
          "value_list": [
            "individual",
            "group",
            "system",
            "organization",
            "class",
            "unknown"
          ]
        },
        "identity_name": {
          "order": 3,
          "required": true,
          "default": "CTIS",
          "description": "Name",
          "data_type": "string"
        }
      },
      "output": [],
      "versions": "EQ(*)"
    },
    {
      "action": "submit bundle to taxii server",
      "description": "Submit STIX bundle to TAXII server",
      "type": "generic",
      "identifier": "submit_bundle_to_taxii_server",
      "read_only": false,
      "parameters": {
        "bundle_id": {
          "order": 0,
          "required": true,
          "description": "Bundle ID which refers to the SOAR container tagged with this ID",
          "data_type": "string"
        },
        "collection_id": {
          "order": 1,
          "required": true,
          "description": "Collection ID to write the bundle to",
          "data_type": "string"
        }
      },
      "output": [
        {
          "data_path": "action_result.data.*.response",
          "data_type": "string"
        }
      ],
      "versions": "EQ(*)"
    },
    {
      "action": "on poll",
      "identifier": "on_poll",
      "description": "Batch submission of IOCs",
      "type": "ingest",
      "read_only": false,
      "parameters": {
        "start_time": {
          "description": "Parameter Ignored in this app",
          "data_type": "numeric",
          "order": 0
        },
        "end_time": {
          "description": "Parameter Ignored in this app",
          "data_type": "numeric",
          "order": 1
        },
        "container_id": {
          "description": "Parameter Ignored in this app",
          "data_type": "numeric",
          "order": 2
        },
        "container_count": {
          "description": "Parameter Ignored in this app",
          "data_type": "numeric",
          "order": 3
        },
        "artifact_count": {
          "description": "Parameter Ignored in this app",
          "data_type": "numeric",
          "order": 4
        }
      },
      "output": [],
      "versions": "EQ(*)"
    }
  ]
}
